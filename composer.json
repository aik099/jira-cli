{
	"name": "aik099/jira-cli",
	"description": "Toolbox for working with Jira from command line",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],
	"require": {
		"symfony/console": "~2.4",
		"pimple/pimple": ">=2.0",
		"stecman/symfony-console-completion": "~0.6@dev",
		"guzzlehttp/guzzle": "~4.0"
	},
	"require-dev": {
